journal = {School Effectiveness and School Improvement},
keywords = {comparative research,data-based school,school performance feedback,teacher,test-based accountability},
number = {2},
pages = {145--165},
title = {{Accountability policies and teachers' acceptance and usage of school performance feedback: A comparative study}},
volume = {21},
