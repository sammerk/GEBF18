@article{Lodewyk2002,
abstract = {A challenge to successful teaching involves knowing how to address students' motivational and cognitive processes in relation to appropriate instruction. Some research has revealed that school tasks are important factors affecting student self-efficacy, motivation, cognition, and participation in instruction and learning. More understanding of the interaction between aspects of motivation, instruction, learning, and performance through a temporal analysis of problem solving in a normal classroom context is needed. This study traced the self-efficacy judgments of an experimental group of 89 grade ten students in a sectarian school as they worked on two differently designed learning tasks in their regular science class. The first task was ill structured. This task gave students significant choices about how to complete it, did not have identifiable steps or sub-goals, required students to search for and gather their own information and resources, and did not include specific criteria for grading. The second task was well structured. It was structured with particular sub-tasks, came with a packet of necessary resources, and included specific grading criteria. The experimental group and a comparison sample of 358 students from four public schools were also administered measures assessing their epistemological beliefs and perceptions of task or course difficulty. The main objectives of the study were to note differences in reported levels of self-efficacy for learning, self efficacy for performance, and epistemological beliefs according to the structure of each task and to study how these related to achievement. It was anticipated that most students would demonstrate significantly lower performance scores, lower and more unstable perceptions of self-efficacy for learning, self-efficacy for performance, and more harmful beliefs about learning and knowledge in the context of an ill-structured task than a well structured task. Self-efficacy for learning was expected to be lower than self-efficacy for performance on both tasks. Epistemological beliefs of the experimental, sample appeared to vary considerably from the comparison group so this variable was not examined further. Other results of the study support some of the hypotheses. Students reported significantly higher levels of self-efficacy for the well-structured task. Academic achievement and self-efficacy for learning were more predictive of achievement on the ill-structured task. Self-efficacy for learning was significantly lower, less stable, and more predictive of task achievement than self-efficacy for performance especially in the early phases of each task. These and other results suggest that features in both tasks may be difficult for students particularly early in task engagement when levels of self-efficacy for performance and learning differ most. Implications about the influence of each measured construct on achievement and impressions of task difficulty were discussed in hopes of bridging the theoretical gap between students' motivational and cognitive processes and constructive learning tasks. (PsycINFO Database Record (c) 2010 APA, all rights reserved)},
author = {Lodewyk, Ken Robert},
journal = {Dissertation Abstracts International Section A: Humanities and Social Sciences},
keywords = {task structure;self efficacy;epistemological belie},
number = {9-A},
title = {{A time-based study of task structure and secondary students' self-efficacy and epistemological beliefs: Implications for achievement and perceptions of task difficulty}},
url = {http://www.redi-bw.de/db/ebsco.php/search.ebscohost.com/login.aspx?direct=true{\&}db=psyh{\&}AN=2002-95005-073{\&}site=ehost-live},
volume = {62},
year = {2002}
}
