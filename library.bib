@article{Kuhn2000,
abstract = {Proposes the coordination of the subjective and objective dimensions of knowing as the essence of what develops in the attainment of mature epistemological understanding. The authors argue that initially, the objective dimension dominates, to the exclusion of subjectivity; subsequently, the subjective dimension assumes an ascendant position and the objective is abandoned, and, finally, the 2 are coordinated. This progression, the authors further postulate, tends to occur in a systematic order across different judgment domains (personal taste, aesthetic, value, and truth), with the orders the reverse of one another in the 2 major transitions that constitute this progression. These predictions are supported in 2 studies among a sample of 7 groups of children, adolescents, and adults varying in age, education, and life experience. Subjectivity is most readily acknowledged in personal taste and aesthetic judgments and least readily in truth judgments. Once subjectivity is accepted and becomes dominant, objectivity is reintegrated in the reverse order, i.e., most readily with respect to truth judgments. Not predicted, however, was the finding that for a number of individuals, both transitions proved most difficult in the values domain. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
author = {Kuhn, Deanna and Cheney, Richard and Weinstock, Michael},
doi = {10.1016/S0885-2014(00)00030-7},
file = {::},
journal = {Cognitive Development},
keywords = {Epistemology,Judgment,Knowle,about the,belief,beliefs about knowing and,cognitive development,corresponding author,determinants,epistemology,it is not surprising,judgement,knowing,knowledge are potentially important,objectivity,of intellectual performance,subjectivity,that what people believe,truth},
