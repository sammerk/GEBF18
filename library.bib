journal = {Computers {\&} Education},
keywords = {Epistemic evaluation,Inquiry tasks,Instructional interventions,Multiple document comprehension,Source evaluation},
pages = {143--157},
publisher = {Elsevier Ltd},
title = {{Epistemic evaluation and comprehension of web-source information on controversial science-related topics: Effects of a short-term instructional intervention}},
url = {http://www.sciencedirect.com/science/article/pii/S0360131514000736},
volume = {76},
year = {2014}
}
@article{Hollands1998,
abstract = {People take longer to judge part-to-whole relationships with bar graphs than with pie charts or divided bar graphs. Ss may perform summation operations to establish the whole with bar graphs, which would be unnecessary for other graph types depicting the whole with a single object. To test this summation model, the number of components forming the whole was varied with bars, divided bars, reference bars, and pies in 3 experiments using a total of 72 Ss. Response time increased with the number of components for bar graphs but there was little increase for other graph types in Exp 1. An accuracy emphasis in Exp 2 produced generally longer response times, but had little effect on the time per summation. The summation operation was not used when graphs were displayed briefly in Exp 3, although subjects still took longer with bars. The estimated time for a summation operation is consistent with estimates derived from other research. In general, the bar graph is not effective for proportion judgments, and its disadvantage becomes potentially greater as the number of components increases.},
author = {Hollands, J. G. and Spence, Ian},
doi = {10.1002/(SICI)1099-0720(199804)12:2<173::AID-ACP499>3.0.CO;2-K},
file = {:Users/samuelmerk/Library/Application Support/Mendeley Desktop/Downloaded/Hollands, Spence - 1998 - Judging proportion with graphs the summation model.pdf:pdf},
