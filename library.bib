@article{Kyriacou2001,
abstract = {Research on teacher stress has become a major area of international research interest. This paper reviews research findings on teacher stress and suggests five directions for future research: (1) monitoring the extent to which particular educational reforms are generating high levels of teacher stress; (2) exploring why some teachers are able to successfully negotiate periods of career reappraisal and retain a positive commitment to their work, whilst others are not; (3) clarifying the nature of the stress process in term of two types of triggers: one based on excessive demands and the other based on a concern with self-image; (4) assessing the effectiveness of particular intervention strategies to reduce teacher stress; and (5) exploring the impact of teacher pupil interaction and classroom climate on teacher stress. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
