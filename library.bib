journal = {Zeitschrift f{\"{u}}r P{\"{a}}dagogik},
keywords = {Bewertung,Deutschland,Deutschunterricht,Grundschule,Indikator,Lehrer,Lernen,Lob,L{\"{a}}ngsschnittuntersuchung,Methode,Pers{\"{o}}nlichkeitsentwicklung,Qualit{\"{a}}t,Quantitative Forschung,Unterrichtsanalyse,Unterrichtsbeobachtung,Unterrichtsforschung,Unterrichtsklima,Unterrichtsst{\"{o}}rung,Validit{\"{a}}t,Verfahren,Verhalten,Videoaufzeichnung},
