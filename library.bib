mendeley-tags = {FAIQ,graphical{\_}perception,statistical{\_}literacy},
number = {6},
pages = {569--582},
pmid = {16950729},
title = {{The influence of graphic format on breast cancer risk communication}},
volume = {11},
year = {2006}
}
