@article{Kennedy2016,
abstract = {Professional development programs are based on different theories of how students learn and different theories of how teachers learn. Reviewers often sort programs according to design features such as program duration, intensity, or the use of specific techniques such as coaches or online lessons, but these categories do not illuminate the programs' underlying purpose or premises about teaching and teacher learning. This review sorts programs according to their underlying theories of action, which include (a) a main idea that teachers should learn and (b) a strategy for helping teachers enact that idea within their own ongoing systems of practice. Using rigorous research design standards, the review identifies 28 studies. Because studies differ in multiple ways, the review presents program effects graphically rather than statistically. Visual patterns suggest that many popular design features are not associated with program effectiveness. Furthermore, different main ideas are not differentially effective. However, the pedagogies used to facilitate enactment differ in their effectiveness. Finally, the review addresses the question of research design for studies of professional development and suggests that some widely favored research designs might adversely affect study outcomes. },
author = {Kennedy, Mary M},
doi = {10.3102/0034654315626800},
file = {:Users/samuelmerk/Library/Application Support/Mendeley Desktop/Downloaded/Kennedy - 2016 - How Does Professional Development Improve Teaching.pdf:pdf},
issn = {0034-6543},
journal = {Review of Educational Research },
number = {4},
pages = {1--36},
title = {{How Does Professional Development Improve Teaching?}},
url = {http://rer.sagepub.com/content/early/2016/01/29/0034654315626800.abstract},
volume = {86},
year = {2016}
}
@incollection{Terhart2011e,
abstract = {Der Autor m{\"{o}}chte zun{\"{a}}chst (1) kurz auf die Entwicklung der Fachdidaktik eingehen, soweit diese in Verbindung zur Erziehungswissenschaft zu sehen ist. Dies ist auch als eine Art Erinnerung an fr{\"{u}}here Diskussionen und Stadien gedacht, so dass auf diese Weise auch die Hintergr{\"{u}}nde der aktuellen Situation deutlich werden. Danach (2) m{\"{o}}chte er sich der Verh{\"{a}}ltnisbestimmung zwischen Erziehungswissenschaft und Fachdidaktiken zuwenden und dies in drei Schritten vollziehen. Es geht zun{\"{a}}chst um konzeptionelle Probleme (2.1), dann um institutionelle Verortungen (2.2) und drittens dann um einige notwendige Perspektiven (2.3). In diesem Kontext m{\"{o}}chte der Autor eine Modellvorstellung darlegen, die die Verh{\"{a}}ltnisse - so wie er sie sieht - verdeutlichen soll. Im dritten Teil (3) geht er n{\"{a}}her auf die aktuelle Situation in der Lehrerbildung ein, denn aufgrund der verschiedenen inhaltlichen und institutionellen Reformprozesse in diesem Bereich ist auch die Situation der Erziehungswissenschaft und der Fachdidaktiken - manchmal zu Bildungswissenschaften zusammengefasst - mit betroffen. Insbesondere die Situation nach dem neuen Lehrerausbildungsgesetz in Nordrhein-Westfalen bringt eine neue Lage, da dort ab 2011 f{\"{u}}r alle Lehr{\"{a}}mter ein einheitlich langes Studium von 10 Semestern vorgesehen ist; insofern handelt es sich angesichts der Geschichte der Lehrerbildung in Deutschland um eine historische Z{\"{a}}sur. (DIPF/Orig.).},
