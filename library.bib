@article{Phan200803,
abstract = {Background: Although extensive research has examined epistemological beliefs, reflective thinking and learning approaches, very few studies have looked at these three theoretical frameworks in their totality. Aims: This research tested two separate structural models of epistemological beliefs, learning approaches, reflective thinking and academic performance among tertiary students over a period of 12 months. Sample: Participants were first-year Arts (N = 616; 271 females, 345 males) and second-year Mathematics (N = 581; 241 females, 341 males) university students. Method: Students' epistemological beliefs were measured with the Schommer epistemological questionnaire (EQ, Schommer, 1990). Reflective thinking was measured with the reflective thinking questionnaire (RTQ, Kember et al., 2000). Student learning approaches were measured with the revised study process questionnaire (R-SPQ-2F, Biggs, Kember, {\{}{\&}{\}} Leung, 2001). LISREL 8 was used to test two structural equation models - the cross-lag model and the causal-mediating model. Results: In the cross-lag model involving Arts students, structural equation modelling showed that epistemological beliefs influenced student learning approaches rather than the contrary. In the causal-mediating model involving Mathematics students, the results indicate that both epistemological beliefs and learning approaches predicted reflective thinking and academic performance. Furthermore, learning approaches mediated the effect of epistemological beliefs on reflective thinking and academic performance. Results of this study are significant as they integrated the three theoretical frameworks within the one study. (PsycINFO Database Record (c) 2010 APA, all rights reserved) (journal abstract)},
author = {Phan, Huy P},
doi = {10.1348/000709907X204354},
issn = {00070998},
journal = {British Journal of Educational Psychology},
keywords = {epistemological beliefs;reflective thinking;learni},
number = {1},
pages = {75--93},
title = {{Predicting change in epistemological beliefs, reflective thinking and learning styles: A longitudinal study}},
url = {http://www.redi-bw.de/db/ebsco.php/search.ebscohost.com/login.aspx?direct=true{\&}db=psyh{\&}AN=2008-01697-004{\&}site=ehost-live},
volume = {78}
}
@article{StrohmKitchener2002,
abstract = {Claims have been made that similar patterns in the development of folk epistemology occur in children, adolescents, and college students. Some researchers claim that children hold absolutistic assumptions about knowledge that evolve into a kind of relativistic framework where personal interpretation is primary, and last into a rationalistic epistemology. Others suggest the same developmental sequence begins in early adolescence or during the college years. The question addressed in this article is how to make sense of these seemingly discrepant claims and the data supporting them. After initially reviewing the alternative models, Fischer's (Psychol. Rev. 87 (1980) 477-531) skill theory is suggested as a framework for understanding the apparent discrepancies that considers both measurement and time of development issues. Fischer and his colleagues distinguish between "functional level" and "optimal level". Functional level is the level at which most people operate without contextual support or practice. It marks the lower end of the development range. He contrasts this to "optimal level" which marks the upper limit of the person's information processing activity. Performance on epistemological tasks may vary between these two levels. Functional and optimal level data from the reflective judgment model (Developing Reflective Judgment: Understanding and Promoting Intellectual Growth and Critical Thinking in Adolescents and Adults, Jossey-Bass, San Francisco, 1994) are used to illustrate the different levels of folk epistemology expressed under these two conditions and different ages at which they appear. The importance of using consistent, structural descriptions of epistemological levels across different models is emphasized for comparison purposes and, again, skill theory is suggested as one alternative framework for describing differences in structure across levels and reinterpreting the data on epistemological assumptions in children, adolescents, and college students. ?? 2002 Elsevier Science Ltd. All rights reserved.},
author = {{Strohm Kitchener}, Karen},
doi = {10.1016/S0732-118X(02)00012-0},
file = {:Users/samuelmerk/Library/Application Support/Mendeley Desktop/Downloaded/Strohm Kitchener - 2002 - Skills, tasks, and definitions Discrepancies in the understanding and data on the development of folk epistemo.pdf:pdf},
isbn = {0732-118X},
issn = {0732118X},
journal = {New Ideas in Psychology},
number = {2-3},
pages = {309--328},
title = {{Skills, tasks, and definitions: Discrepancies in the understanding and data on the development of folk epistemology}},
volume = {20},
year = {2002}
}
