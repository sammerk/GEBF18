@article{Lahtinen2013,
abstract = {The study describes the advancement of the epistemological beliefs of university students. After obtaining interesting research results on changes in students? (N = 170) conceptions of learning from Jan Meyer?s Reflections on Learning Inventory, we carried out a qualitative study using a focus group interview of seven students. The purpose was to gain a deeper understanding of the nature of changes in students? conceptions of learning. However, the initial analysis of the data directed our attention to the epistemological aspects, and we continued the analysis by applying Bendixen and Rule?s Integrative Personal Epistemology Model. The students? educational experiences showed that their epistemological beliefs changed over a period of time from absolutism towards relativism and that the pedagogical environment played an important role in this process. On the basis of the study, we suggest that it is already possible to influence the advancement of university students? epistemological beliefs at an early stage in their studies.},
annote = {doi: 10.1080/0309877X.2011.645458},
author = {Lahtinen, Aino-Maija and Pehkonen, Leila},
doi = {10.1080/0309877X.2011.645458},
issn = {0309-877X},
journal = {Journal of Further and Higher Education},
keywords = {FAIQ},
mendeley-tags = {FAIQ},
month = {may},
number = {3},
pages = {397--415},
publisher = {Routledge},
title = {{‘Seeing things in a new light,: conditions for changes in the epistemological beliefs of university students}},
url = {http://dx.doi.org/10.1080/0309877X.2011.645458},
volume = {37},
year = {2013}
}
@article{Gill2004,
abstract = {We investigated a theoretical model including an instructional intervention and systematic processing to account for change in preservice teachers' epistemological beliefs about teaching and learning in mathematics. General and subject-specific epistemological beliefs and systematic processing were assessed in 161 preservice teachers, randomly assigned to an experimental group whose epistemological beliefs about mathematics were activated and challenged through augmented activation and refutational text or to a control group who read a traditional expository text. The model was partially supported. The treatment group receiving the instructional intervention demonstrated greater change in implicit epistemological beliefs than the control group, and partial support for systematic processing as a mediator of the relationship between general epistemological beliefs and change in specific epistemological beliefs was obtained. ?? 2004 Elsevier Inc. All rights reserved.},
author = {Gill, Michele Gregoire and Ashton, Patricia T. and Algina, James},
doi = {10.1016/j.cedpsych.2004.01.003},
isbn = {0361-476X},
issn = {0361-476X},
journal = {Contemporary Educational Psychology},
keywords = {Augmented activation,Conceptual change,Epistemological beliefs,Mathematics,Refutational text,Systematic processing},
number = {2},
pages = {164--185},
title = {{Changing preservice teachers' epistemological beliefs about teaching and learning in mathematics: An intervention study}},
volume = {29},
year = {2004}
}
