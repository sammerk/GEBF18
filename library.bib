journal = {PLoS Biology},
number = {5},
pages = {1--15},
pmid = {27171007},
title = {{Badges to Acknowledge Open Practices: A Simple, Low-Cost, Effective Method for Increasing Transparency}},
volume = {14},
year = {2016}
}
