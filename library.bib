journal = {School Psychology Quarterly},
keywords = {multilevel modeling referrals student behavior pro},
number = {1},
pages = {13--27},
title = {{A multilevel exploration of the influence of teacher efficacy and burnout on response to student problem behavior and school-based service use}},
