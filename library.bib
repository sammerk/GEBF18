journal = {Journal of Educational Psychology},
keywords = {2009,academic performance,and aca-,and spe-,blazer,both from a general,cific areas such as,homework,mathematics,multilevel models,of homework have been,perspective,science,studied,the characteristics and effects,the relationship between homework,widely},
number = {4},
pages = {No Pagination Specified},
title = {{Adolescents' Homework Performance in Mathematics and Science: Personal Factors and Teaching Practices.}},
volume = {107},
year = {2015}
}
