journal = {Patient Education and Counseling},
keywords = {FAIQ,Graphical format,Medical decision-making,Quantitative information,Risk communication,Risk perception,graphical{\_}perception},
mendeley-tags = {FAIQ,graphical{\_}perception},
number = {1-3},
pages = {121--128},
pmid = {17905553},
title = {{Communicating risk information: The influence of graphical display format on quantitative information perception-Accuracy, comprehension and preferences}},
volume = {69},
