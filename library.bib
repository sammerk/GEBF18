arxivId = {NIHMS150003},
author = {Brown, Sandra M. and Culver, Julie O. and Osann, Kathryn E. and MacDonald, Deborah J. and Sand, Sharon and Thornton, Andrea A. and Grant, Marcia and Bowen, Deborah J. and Metcalfe, Kelly A. and Burke, Harry B. and Robson, Mark E. and Friedman, Susan and Weitzel, Jeffrey N.},
