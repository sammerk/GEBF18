issn = {1939-2176},
journal = {Journal of Educational Psychology},
keywords = {1997,2008,bosker,en-,gagement and learning,hattie,instructional quality is believed,instructional variability,math self-concept,quality,repeated measurements,scheerens,standardized math achievement and,students,teacher-student ratings of instructional,to be crucial for},
