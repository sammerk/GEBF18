author = {Uhlmann, Stefan and Priemer, Burkhard},
file = {::},
journal = {PhyDid B - Didaktik der Physik - Beitr{\"{a}}ge zur DPG-Fr{\"{u}}hjahrstagung; 2010: Hannover},
