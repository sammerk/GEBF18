journal = {Health Expectations},
keywords = {FAIQ,communication,genetic,genomic,graphical{\_}perception,melanoma,public preferences,risk,statistical{\_}literacy},
mendeley-tags = {FAIQ,graphical{\_}perception,statistical{\_}literacy},
