keywords = {Fallstudie,Lernbedingungen,Lernmot,Lernpsychologie},
number = {3},
pages = {473--504},
title = {{Fachdidaktische Massnahmen im Franz{\"{o}}sisch-Unterricht aus Sch{\"{u}}lersicht: Der Zusammenhang mit Schulleistung und Motivation}},
