doi = {10.1080/02664763.2013.818625},
file = {:Users/samuelmerk/Library/Application Support/Mendeley Desktop/Downloaded/Wooff, Jamalzadeh - 2013 - Robust and scale-free effect sizes for non-Normal two-sample comparisons, with applications in e-commerce.pdf:pdf},
isbn = {10.1080/02664763.2013.818625},
issn = {02664763},
keywords = {FAIQ,Weibull,data{\_}literacy,e-commerce,effect size,long-tail distribution,non-Normal distribution,quantile function,two-sample comparison},
mendeley-tags = {FAIQ,data{\_}literacy},
