number = {4},
pages = {1560--1574},
publisher = {Behavior Research Methods},
title = {{Effect size measures in a two-independent-samples case with nonnormal and nonhomogeneous data}},
url = {http://dx.doi.org/10.3758/s13428-015-0667-z},
volume = {48},
year = {2016}
