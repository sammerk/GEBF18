@article{MacKinnon2000,
abstract = {This paper describes the statistical similarities among mediation, confounding, and suppression. Each is quantified by measuring the change in the relationship between an independent and a dependent variable after adding a third variable to the analysis. Mediation and confounding are identical statistically and can be distinguished only on conceptual grounds. Methods to determine the confidence intervals for confounding and suppression effects are proposed based on methods developed for mediated effects. Although the statistical estimation of effects and standard errors is the same, there are important conceptual differences among the three types of effects.},
author = {MacKinnon, David P and Krull, Jennifer L and Lockwood, Chondra M},
issn = {1389-4986},
journal = {Prevention science: the official journal of the Society for Prevention Research},
month = {dec},
number = {4},
pages = {173},
title = {{Equivalence of the Mediation, Confounding and Suppression Effect}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2819361/},
volume = {1},
year = {2000}
}
@article{Krettenauer2005,
abstract = {The study investigates whether the strategy of translating an interview procedure into a standardized questionnaire proven and tested in the field of moral development by Rest (1979) can be transferred to other research areas of developmental psychology. To this end, a newly developed questionnaire for measuring the developmental level of epistemological beliefs (Fragebogen zur Erfassung des Entwicklungsniveaus epistemologischer {\"{U}}berzeugungen - FREE) is presented that is based on similar construction principles as the Defining Issues Test (DIT). The reliability and validity of this questionnaire are tested. Cross-sectional and longitudinal analyses demonstrate that the FREE adequately reflects effects of age and education on the development of epistemological beliefs. Furthermore, a substantive correlation between questionnaire and interview data was found. At the same time, however, questionnaire and interview procedures yielded quite different absolute estimates of individuals' competence level. Results are highly consistent with findings obtained in the field of moral development. The FREE, thus, provides a viable alternative to interview procedures for measuring the developmental level of epistemological beliefs as far as the focus is on individual differences rather than individuals' absolute competence level.$\backslash$n$\backslash$nAbstract in German$\backslash$n$\backslash$nZusammenfassung. Es wird untersucht, inwieweit die von Rest (1979) im Bereich der Moralentwicklung erprobte Strategie der {\"{U}}bersetzung von Interviewmethoden in Fragebogenverfahren auf andere Bereiche entwicklungspsychologischer Forschung {\"{u}}bertragbar ist. Hierf{\"{u}}r wird ein neu entwickelter Fragebogen zur Erfassung des Entwicklungsniveaus epistemologischer {\"{U}}berzeugungen (FREE) vorgestellt, der auf vergleichbaren Konstruktionsprinzipien beruht wie der Defining Issues Test (DIT). F{\"{u}}r diesen Fragebogen werden Reliabilit{\"{a}}ts- und Validit{\"{a}}tskennwerte ermittelt. Quer- und l{\"{a}}ngsschnittliche Analysen weisen eine ausreichende Sensitivit{\"{a}}t des FREE gegen{\"{u}}ber Alters- und Bildungseffekten nach. Zudem ergab sich eine hohe korrelative {\"{U}}bereinstimmung von Fragebogen- und Interviewdaten. Eine deutliche Differenz zwischen beiden Methoden zeigte sich jedoch bei der Einsch{\"{a}}tzung des absoluten Kompetenzniveaus der Befragten. Die Ergebnisse entsprechen weitgehend den Befunden im Bereich der Moralentwicklung und sprechen f{\"{u}}r die Generalisierbarkeit der {\"{U}}bertragungsstrategie. Der FREE empfiehlt sich folglich dann als Alternative zu Interviewverfahren, wenn bei der Untersuchung des Entwicklungsniveaus epistemologischer {\"{U}}berzeugungen interindividuelle Unterschiede und nicht das absolute Kompetenzniveau im Vordergrund stehen.},
author = {Krettenauer, Tobias},
doi = {10.1026/0049-8637.37.2.69},
file = {::},
journal = {Zeitschrift f{\"{u}}r Entwicklungspsychologie und p{\"{a}}dagogische Psychologie},
keywords = {Epistemological beliefs,Measurement},
number = {2},
pages = {69--79},
title = {{Die Erfassung des Entwicklungsniveaus epistemologischer {\"{U}}berzeugungen und das Problem der {\"{U}}bertragbarkeit von Interviewverfahren in standardisierte Fragebogenmethoden}},
volume = {37},
year = {2005}
}
@article{Zuell2015,
abstract = {This article investigates item nonresponse in open-ended survey questions because such item nonresponse is much higher than in closed questions. The difference is a result of the higher cognitive burden placed on the respondent. To study item nonresponse, we manipulate different questionnaire design characteristics, such as the size of the answer box and the inclusion of motivation texts, as well as respondent-specific characteristics, in a randomized web experiment using a student sample. The results show that a motivation text increases the frequency of responses to open-ended questions for both small and large answer boxes. However, large answer boxes earn higher item nonresponse than small answer boxes regardless of the usage of a motivation text. In addition, gender and the respondent's field of study affected the answering of open-ended questions; being a woman or studying social sciences increased the frequency of a response. As the major finding and in contrast to previous findings, our results indicate that particularly large answer boxes should be avoided, because they reduce respondents' willingness to respond.},
author = {Zuell, Cornelia and Menold, Natalja and Korber, Sabine},
doi = {10.1177/0894439314528091},
issn = {0894-4393},
journal = {Social Science Computer Review},
keywords = {FAIQ,although many surveys use,answer box size,demographic variables,helpful to gain additional,item nonresponse,mation from respondents,more sophisticated infor-,motivation,open-ended answers,open-ended questions,open-ended questions in combination,questions in surveys is,the use of open-ended,with},
mendeley-tags = {FAIQ,open-ended answers},
number = {1},
pages = {115--122},
title = {{The influence of the answer box size on item nonresponse to open-ended questions in a web survey}},
volume = {33},
year = {2015}
}
@article{Reeves2017,
abstract = {ABSTRACTIn recent years, the use of data to inform instructional decision making has become a particularly prominent facet of K–12 educators' professional practice. However, research estimates limited opportunities for preservice teachers to learn how to use data, including standardized test data, for such purposes. In response, this article describes the results of a pretest–posttest study of a 6-hour standardized assessment data use intervention for preservice teachers. The facilitated, collaborative, and highly structured assessment course-embedded intervention engaged preservice teachers in asking and answering four different kinds of questions (e.g., achievement strengths and weaknesses, instructional implications) at five different student levels (e.g., individual, subgroup, school) with external assessment data presented in tables, charts, and score reports. Findings—which include highly favorable preservice teacher perceptions of the intervention's impact on their data-driven decision making skill...},
author = {Reeves, Todd D. and Chiang, Jui Ling},
doi = {10.1080/08878730.2016.1273420},
issn = {19388101},
journal = {Teacher Educator},
number = {2},
pages = {155--172},
publisher = {Taylor {\&} Francis},
title = {{Building Pre-service Teacher Capacity to Use External Assessment Data: An Intervention Study}},
url = {http://dx.doi.org/10.1080/08878730.2016.1273420},
